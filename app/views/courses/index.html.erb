<div class="row">
  <br/>

  <% @courses.each do |course| %>
      <%= link_to course do %>

      <div class="col s12 m4">
        <div class="card">

          <div class="card-image waves-effect waves-block waves-light">
            <%= image_tag course.image %>
          </div>

          <div class="card-content">
            <span class="card-title grey-text text-darken-4 ">
            <%= course.name %>
            </span>
          </div>

        </div>
      </div>
    <% end %>
  <% end %>
</div>
